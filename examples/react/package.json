{
  "name": "@arcanejs/examples-react",
  "version": "0.0.3",
  "private": true,
  "description": "Example app using toolkit",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "tsc --noEmit",
    "start": "node --import=tsx --conditions=@arcanejs/source",
    "start:button-adder": "pnpm start src/button-adder",
    "start:counter": "pnpm start src/counter",
    "start:editable-group-title": "pnpm start src/editable-group-title",
    "start:readme": "pnpm start src/readme",
    "start:reorder-elements": "pnpm start src/reorder-elements",
    "start:tabs-colors-inputs": "pnpm start src/tabs-colors-inputs"
  },
  "devDependencies": {
    "@arcanejs/eslint-config": "workspace:^",
    "@arcanejs/typescript-config": "workspace:^",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "^18",
    "eslint": "^8.57.0",
    "tsup": "^8.1.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@arcanejs/toolkit": "workspace:^",
    "@arcanejs/react-toolkit": "workspace:^",
    "react": "^18",
    "tsx": "^4.19.1"
  }
}